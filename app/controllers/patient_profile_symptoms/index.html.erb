<div class="container">





<p id="notice"><%= notice %></p>

<h1>Current or Recent Symptoms</h1>

<table class="table table-striped">
  <thead>
  <tr>
    <th>symptom description</th>
    <th>Operations</th>
  </tr>
  </thead>

  <tbody>
  <% @profile_symptoms.each do |i| %>
      <tr >
        <td><%= i.symptom_description%></td>
        <td><%= link_to 'Show', i%>
          <%= link_to 'Edit', edit_patient_profile_symptom_path(i) %>
          <%= link_to 'Destroy', patient_profile_symptoms_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>



<%= link_to 'New Record', new_patient_profile_symptom_path,class: "btn  btn-success " %>
  <%= link_to 'Cancel', intake_index_path,class: "btn  btn-warning " %>
</div>